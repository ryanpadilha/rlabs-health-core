{
	"package": {
		"name": "vulcano-health-core",
		"repo": "maven",
		"desc": "Vulcano Health Core Library API Monitor for Microservices",
		"vcs_url": "https://github.com/ryanpadilha/vulcano-health-core.git",
		"github_use_tag_release_notes": true,
		"licenses": [
			"apache-2.0"
		],
		"public_download_numbers": false,
		"public_stats": false
	},
	"version": {
		"name": "${project.version}",
		"vcs_tag": "${project.version}",
		"released": "${build.timestamp}"
	},
	"files": [
		[
			{
				"includePattern": "target/(.*)",
				"excludePattern": "target/.*.tar.gz$",
				"uploadPattern": "/$1"
			}
		]
	],
	"publish": true
}